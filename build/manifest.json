{
  "manifest_version": 2,
  "name": "New Chrome Extension",
  "description": "set new description",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "alarms"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["js/cs-bundle.js"]
    }
  ],
  "background": {
    "scripts": ["js/bg-bundle.js"]
  }
}